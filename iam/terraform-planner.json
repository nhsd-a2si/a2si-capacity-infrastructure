{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListAllMyBuckets",
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "s3:GetBucketWebsite"
            ],
            "Resource": [
                "arn:aws:s3:::*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetBucketWebsite"
            ],
            "Resource": [
                "arn:aws:s3:::digim-tf-assets"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectTagging"
            ],
            "Resource": [
                "arn:aws:s3:::*-certificates/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "route53:ListHostedZones"
            ],
            "Resource": [
                "*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "route53:GetHostedZone"
            ],
            "Resource": [
                "*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "route53:ListResourceRecordSets"
            ],
            "Resource": [
                "arn:aws:route53:::hostedzone/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:GetUser"
            ],
            "Resource": [
                "arn:aws:iam::962310933334:user/terraform"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:ListInstanceProfiles",
                "iam:ListInstanceProfilesForRole"
            ],
            "Resource": [
                "arn:aws:iam::962310933334:instance-profile/*",
                "arn:aws:iam::962310933334:role/*-ec2-role"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "elasticbeanstalk:ListAvailableSolutionStacks"
            ],
            "Resource": [
                "arn:aws:elasticbeanstalk:eu-west-2::solutionstack/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "elasticbeanstalk:DescribeApplications"
            ],
            "Resource": [
                "arn:aws:elasticbeanstalk:eu-west-2:962310933334:application/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "elasticbeanstalk:DescribeConfigurationSettings"
            ],
            "Resource": [
                "arn:aws:elasticbeanstalk:eu-west-2:962310933334:configurationtemplate/*/*",
                "arn:aws:elasticbeanstalk:eu-west-2:962310933334:environment/*/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "acm:DescribeCertificate"
            ],
            "Resource": [
                "arn:aws:acm:eu-west-2:962310933334:certificate/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:GetInstanceProfile"
            ],
            "Resource": [
                "arn:aws:iam::962310933334:instance-profile/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:ListAttachedRolePolicies"
            ],
            "Resource": [
                "arn:aws:iam::962310933334:role/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "elasticache:DescribeCacheSubnetGroups",
                "elasticache:DescribeCacheClusters",
                "elasticache:DescribeReplicationGroups"
            ],
            "Resource": [
                "*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "rds:DescribeDBSubnetGroups",
                "rds:ListTagsForResource"
            ],
            "Resource": [
                "arn:aws:rds:eu-west-2:962310933334:subgrp:*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "rds:DescribeDBInstances",
                "rds:ListTagsForResource"
            ],
            "Resource": [
                "arn:aws:rds:eu-west-2:962310933334:db:*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:GetItem",
                "dynamodb:PutItem",
                "dynamodb:DeleteItem"
            ],
            "Resource": [
                "arn:aws:dynamodb:eu-west-2:962310933334:table/*-state"
            ]
        }
    ]
}